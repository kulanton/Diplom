%h2 Поиск
= form_tag search_path, :class => 'well form-search', :method => 'get', :id => 'search' do 
  %fieldset
    = text_field_tag :search, params[:search], :class => 'input-medium search-query'
    = submit_tag 'Поиск', :name => nil, :class => 'btn'

-#- if @examines
#examines
-#= render 'examines'

-#- if @scripts
#scripts
-#= render 'scripts'

.free-space
